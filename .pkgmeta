package-as: UICore
enable-nolib-creation: no

embedded-libraries:
    - ace3
    - libactionbutton-1-0
    - libkeybound-1-0

optional-dependencies:
    - ace3
    - clique
    - libkeybound-1-0

tools-used:
    - ouf

move-folders:
    UICore/UICore: UICore

manual-changelog:
    filename: CHANGELOG.md
    markup-type: markdown

license-output: LICENSE.txt

ignore:
    - Documentation
    - CHANGELOG.md
    - CODE_OF_CONDUCT.md
    - LICENSE.md
    - README.md
